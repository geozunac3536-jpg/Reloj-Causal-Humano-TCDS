  "about": [
    {
      "@type": "DefinedTerm",
      "name": "Teoría Cromodinámica Sincrónica (TCDS)",
      "termCode": "TCDS",
      "description": "Marco teórico que postula la dinámica Q–Σ–φ–χ como ley universal de coherencia y persistencia ontológica sobre un sustrato discreto (CGA)."
    },
    {
      "@type": "DefinedTerm",
      "name": "Reloj Causal Humano",
      "description": "Nodo sensor ejecutado en un dispositivo móvil que mide aceleración, deriva de fase y entropía de Shannon para estimar el tiempo causal t_C y Σ-metrics locales.",
      "termCode": "RCH-TCDS"
    },
    {
      "@type": "DefinedTerm",
      "name": "Filtro de Honestidad (E-Veto)",
      "description": "Criterio de validación que exige simultáneamente locking de fase alto (LI, R) y caída forzada de entropía (ΔH < umbral) para aceptar una señal como coherente Q-driven, evitando apofenia.",
      "termCode": "E-Veto"
    },
    {
      "@type": "DefinedTerm",
      "name": "Sistema Predictivo Sísmico TCDS",
      "description": "Aplicación de TCDS a la sismología, donde Σ-metrics y t_C se usan para discriminar ruido de precursores y réplicas mediante nodos humanos y estaciones instrumentales.",
      "termCode": "TCDS-SPS"
    }
  ],

